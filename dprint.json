{
  "incremental": true,
  "lineWidth": 160,
  "includes": ["**/*.{ts,tsx,js,jsx,json,md}"],
  "excludes": [
    "common",
    "**/node_modules",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.55.0.wasm",
    "https://plugins.dprint.dev/json-0.7.0.wasm",
    "https://plugins.dprint.dev/markdown-0.10.0.wasm"
  ]
}
